<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name = "com.example.demo.model.QA" table = "qa">
        <id name = "id" type="int" column = "qaid">
            <generator class="increment"/>
        </id>
        <property name="username" type="string" column="qausername"/>
        <property name="password" type="string" column="qapassword" />
    </class>
</hibernate-mapping>